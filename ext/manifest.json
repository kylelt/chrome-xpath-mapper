{
  "name": "Chrome XPath Mapper",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "generates a json key value object of xpath queries based on how you interact with gui",
  "homepage_url": "https://www.google.com.au/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "options_page": "src/options/index.html",
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Xpath Mapper",
    "default_popup": "src/page_action/page_action.html"
  },
  "content_scripts": [
      {    
      "matches": [
        "https://www.google.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}
